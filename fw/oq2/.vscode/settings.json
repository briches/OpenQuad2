    "C_Cpp.default.includePath": [
        "${workspaceFolder}",
        "${workspaceFolder}/application",
        "${workspaceFolder}/application/config",
        "${workspaceFolder}/application/sensor_thread",
        "${workspaceFolder}/application/sensor_thread/platform",
        "${workspaceFolder}/application/sys",
        "${workspaceFolder}/application/freertos",
        "${workspaceFolder}/Drivers/DebugLog",
        "${workspaceFolder}/Drivers/CMSIS/Include/**",
        "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32H7xx",
        "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32H7xx/**",
        "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32H7xx/Include",
        "${workspaceFolder}/Drivers/Mems/**",
        "${workspaceFolder}/Drivers/STM32H7xx_HAL_Driver/Inc",
        "${workspaceFolder}/FATFS/**",
        "${workspaceFolder}/Middlewares/ST/STM32_USB_Device_Library/**",
        "${workspaceFolder}/Middlewares/Third_Party/FatFs/src",
        "${workspaceFolder}/Middlewares/Third_Party/FatFs/src/option",
